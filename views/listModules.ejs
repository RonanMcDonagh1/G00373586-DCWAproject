<table><!-- Create table-->
    <tr>
    <th>Module ID</th>
    <th>Name</th>
    <th>Credits</th>
    <th>Update</th>
    <th>Students</th>
    </tr>
    <% modules.forEach((module) => { %><!-- Itterates through the modules in the database's table to assign the below values to every module -->
    <tr>
    <td><%= module.mid %></td>
    <td><%= module.name %></td>
    <td><%= module.credits %></td>
    <td><a href="http://localhost:3004/module/edit/:mid">Update</a></td>
    <td><a href="http://localhost:3004/module/edit">List</a></td>
    </tr>
    <% }) %>
    </table>

    <a href="http://localhost:3004/">Home</a><!--home link -->